{
  "name": "@pluralsight/icons",
  "version": "0.0.0",
  "description": "Icons for Pluralsight.",
  "main": "build/common/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/*"
  ],
  "exports": {
    "./svg/": "./build/svg/"
  },
  "scripts": {
    "build": "echo \"no build command specified\"",
    "build:svg": "yarn run clean:svg && svgo -r -f src/svg -o build/svg",
    "clean": "rm -rf build",
    "clean:svg": "rm -rf build/svg",
    "test": "echo \"'yarn test' should be run from root directory.\" && exit 1"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight icons",
    "design system",
    "design icons"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/tva/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:pluralsight/tva.git",
    "directory": "packages/icons"
  },
  "license": "Apache 2.0",
  "packageManager": "yarn@3.1.0",
  "devDependencies": {
    "@types/svgo": "^2",
    "svgo": "^2.8.0"
  }
}
