{
  "name": "@pluralsight/headless-styles",
  "version": "0.0.0",
  "description": "Headless styles for Pluralsight.",
  "main": "npm/index.js",
  "module": "npm/browser.js",
  "types": "npm/node/types/headless-styles/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./npm/browser.js",
      "require": "./npm/index.js"
    },
    "./types": {
      "types": "./npm/node/types/headless-styles/src/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "npm/node/types/headless-styles/src/types.d.ts"
      ]
    }
  },
  "files": [
    "npm/**/*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "workspaces": [
    "sandbox"
  ],
  "scripts": {
    "build": "yarn build:common && yarn build:es",
    "build:common": "tsc --project tsconfig.build.json",
    "build:es": "tsc --project tsconfig.browser.json",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "test": "echo \"'yarn test' should be run from root directory.\" && exit 1"
  },
  "dependencies": {
    "kebab-case": "^1.0.1",
    "tslib": "latest"
  },
  "devDependencies": {
    "@pluralsight/design-tokens": "experimental",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.14",
    "@types/react": "latest",
    "identity-obj-proxy": "^3.0.0",
    "typescript": "4.7.4"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight headless styles",
    "headless",
    "styles",
    "headless-styles"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/tva/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:pluralsight/tva.git",
    "directory": "packages/headless-styles"
  },
  "license": "Apache 2.0",
  "packageManager": "yarn@3.2.1"
}
